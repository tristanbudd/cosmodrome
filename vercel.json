{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    },
    {
      "src": "^/css/(.*)$",
      "dest": "/public/css/$1"
    },
    {
      "src": "^/js/(.*)$",
      "dest": "/public/js/$1"
    },
    {
      "src": "^/img/(.*)$",
      "dest": "/public/img/$1"
    },
    {
      "src": "^/img/(.*)$",
      "dest": "/public/icons/$1"
    },
    {
      "src": "^/fonts/(.*)$",
      "dest": "/public/fonts/$1"
    },
    {
      "src": "^/favicon/(.*)$",
      "dest": "/public/favicon/$1"
    },
    {
      "src": "^/lang/(.*)$",
      "dest": "/public/lang/$1"
    },
    {
      "src": "^/themes/(.*)$",
      "dest": "/public/themes/$1"
    },
    {
      "src": "^/robots.txt$",
      "dest": "/public/robots.txt"
    },
    {
      "src": "^/sitemap.xml$",
      "dest": "/public/sitemap.xml"
    },
    {
      "src": "^(.*)$",
      "dest": "/api/"
    }
  ]
}